{% extends 'redactor/base.html.twig' %}

{% block title %}{{formName}}{% endblock title %}

{% block body %}

    {{ parent() }}

    <div class="col-10 p-3">
        {{ form_start(form) }}
            {{ form_row(form.title) }}
            <p>Nous vous rappelons que les images doivent être hébergé sur un site spécialisé</p>
            {{ form_row(form.content) }}
            {{ form_row(form.date) }}
            {{ form_row(form.save) }}
        {{ form_end(form) }}
    </div>

{% endblock %}